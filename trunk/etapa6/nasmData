		SECTION .data
fmtStr:	db	"%s", 0	; The printf format for string
fmtInt:	db	"%d", 0	; The printf format for integer
fmtCha:	db	"%c", 0	; The printf format for character
fmtFlo:	db	"%f", 0	; The printf format for floating
